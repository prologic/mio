# An object is always truish

Object set_slot("and", method(
  eval_arg(0)
))
Object set_slot("or", method(
  self
))

# ... except None and False which are False

None set_slot("and", None)
None set_slot("or", method(
  eval_arg(0)
))

False set_slot("and", False)
False set_slot("or", method(
  eval_arg(0)
))
