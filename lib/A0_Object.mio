Object do (
    "" = method(call message arg(0))
)

True do(
	or = True
    not = False
    ifTrue = method(call message arg(0); None)
)

False do(
	and = False
	or = method(call message arg(0) and True)
	not = True
    ifTrue = False
)

None do(
	not = True
	and = False
	or = method(call message arg(0) and True)
    ifTrue = None
)
