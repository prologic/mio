Object do (
    "" = method(call message arg(0))
)

True do(
	or = True
    not = False
    else = True
    elif = True
    then = method(call message arg(0); None)
)

False do(
	and = False
	or = method(v, if(v))
	not = True
    then = False
    else = method(call message arg(0); None)
    elif = Object get("if")
)

None do(
	not = True
	and = False
	or = method(v, if(v))
    then = None
    else = None
    elif = None
)
