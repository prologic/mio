[info]
title = Parser problem with assignment and complex expressions
owned-by = 
created-by = prologic
assigned-to = 
description = >>> x = parse(tokenize("x = x + 1"))
	make_chain: [x, =, x, +, 1]
	reshuffle: [x, =, x, +, 1]
	[+]
	[x, =, x, 1]
	[(x, +), (=, None), (x, None), (1, None)]
	 root: x +(=)

